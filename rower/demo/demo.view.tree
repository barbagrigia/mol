$mol_rower_demo $mol_rower
	heightMinimal 50
	childs /
		< titler $mol_stringer
			hint \Title
			value > title \
		< counter $mol_number
			hint \Count
			value > count null
		< progresser $mol_portioner
			portion < progress 0.33
		< publisher $mol_checker
			childs / \Shared
			checked > publish false
		< buttonDrop $mol_clicker_minor
			childs / \Drop
			eventClick > eventLog null
